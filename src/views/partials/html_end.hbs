
        <!-- Add your site or application scripts here -->

        <!-- Google Analytics -->
        {{#isTruthy @root.trackingConfig.enable}}
            <script>
                var gaScriptSrc = window.location.hostname === "{{ @root.trackingConfig.prodDomain }}" ? "{{ @root.trackingConfig.source.prod }}" : "{{ @root.trackingConfig.source.dev }}";
                (function(i, s, o, g, r, a, m) {
                    i["GoogleAnalyticsObject"] = r;
                    i[r] = i[r] || function() { (i[r].q = i[r].q || []).push(arguments); }, i[r].l = 1 * new Date();
                    a = s.createElement(o), m = s.getElementsByTagName(o)[0];
                    a.async = 1;
                    a.src = g;
                    m.parentNode.insertBefore(a, m);
                })(window, document, "script", gaScriptSrc, "ga");

                ga("create", "{{ @root.trackingConfig.id }}");
                ga("send", "pageview");
            </script>
        {{/isTruthy}}
    </body>
</html>
