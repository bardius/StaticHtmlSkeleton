{
  "name": "static-html-skeleton",
  "title": "Static Html Skeleton",
  "description": "A static website generator with integrated Zurb Foundation 6.4",
  "version": "4.0.0",
  "homepage": "https://github.com/bardius/StaticHtmlSkeleton",
  "authors": [
    {
      "name": "George Bardis",
      "url": "http://www.bardis.info"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/bardius/StaticHtmlSkeleton"
  },
  "bugs": {
    "url": "https://github.com/bardius/StaticHtmlSkeleton/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "keywords": [],
  "scripts": {
    "setup": "npm install --no-optional",
    "mkdir:checkstyle": "mkdir reports/checkstyle",
    "build": "webpack --config ./configs/webpack.dev.config.babel.js",
    "release": "webpack --config ./configs/webpack.prod.config.babel.js -p",
    "start": "webpack-dev-server --watch --config ./configs/webpack.dev.server.config.babel.js",
    "serve": "budo --live --port 9001 --dir public_html",
    "serve:build": "npm-run-all build serve",
    "serve:release": "npm-run-all release serve",
    "lint:js": "eslint \"src/**/*.js\" -f table --fix --color",
    "lint:css": "stylelint \"src/**/*.scss\" -q --custom-formatter=./node_modules/stylelint-formatter-table --color --max-warnings=30",
    "lint:test": "eslint -c specs/.eslintrc \"specs/**/*.spec.js\" -f table --fix --color",
    "lint": "npm-run-all lint:css lint:test lint:js",
    "test:watch": "jest --config=specs/jest.config.json --watch --color",
    "test:watchAll": "jest --config=specs/jest.config.json --watchAll --color",
    "test:unit": "jest --config=specs/jest.config.json --color",
    "test": "npm-run-all lint test:unit",
    "doc": "jsdoc -c ./configs/jsdoc.config.json -R README.md",
    "checkstyle:js": "eslint \"src/**/*.js\" -f checkstyle -o reports/checkstyle/eslint.xml || true",
    "checkstyle:css": "stylelint \"src/**/*.scss\" -q --custom-formatter=./node_modules/stylelint-checkstyle-formatter 2>&1 | tee reports/checkstyle/stylelint.xml",
    "checkstyle": "npm-run-all checkstyle:js checkstyle:css",
    "check:npm": "./node_modules/.bin/npm-check -s --color || true",
    "check:node": "babel-node ./configs/check-node-version.babel.js",
    "check:stylelint": "stylelint-config-prettier-check",
    "check:eslint": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "pretty": "pretty-quick",
    "pretty:stagged": "pretty-quick --staged",
    "postinstall": "npm-run-all check:node check:npm",
    "precommit": "npm-run-all pretty:stagged test",
    "travis": "grunt travis --verbose"
  },
  "jestSonar": {
    "reportPath": "./reports/unit-tests/jest",
    "reportFile": "results-report.xml"
  },
  "dependencies": {
    "foundation-datepicker": "^1.5.6",
    "foundation-sites": "^6.4.3",
    "jquery": "^3.3.1",
    "motion-ui": "^2.0.3",
    "what-input": "^5.1.2"
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-jscript": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "budo": "^11.3.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "es6-promise-promise": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "handlebars-helpers": "^0.10.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^23.6.0",
    "jest-each": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^12.0.0",
    "jshint-stylish": "^2.2.1",
    "json-loader": "^0.5.7",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-filter-gradient": "^1.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.0.7",
    "postcss-pxtorem": "^4.0.1",
    "postcss-syntax": "^0.34.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "semver": "^5.5.1",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-suitcss": "^14.0.0",
    "stylelint-formatter-pretty": "^1.0.3",
    "stylelint-formatter-table": "^1.0.4",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-stream": "^5.1.1",
    "yargs": "^12.0.2"
  }
}
